{
  "ignorePatterns": [
    {
      "pattern": "^\\./"
    },
    {
      "pattern": "^\\.\\./"
    },
    {
      "pattern": "^https?://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://blog.sa2taka.com/"
    }
  ],
  "timeout": "20s",
  "retryCount": 2,
  "aliveStatusCodes": [
    200,
    206,
    403,
    406,
    429
  ]
}
